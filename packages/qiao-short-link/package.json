{
  "name": "qiao-short-link",
  "version": "3.0.1",
  "description": "URL Shortener on Node.js and Browser",
  "keywords": [
    "permanent",
    "tiny",
    "short",
    "link",
    "url",
    "nodejs",
    "browser"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.insistime.com/qiao-short-link",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-z.git"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "ava __tests__/test.js"
  },
  "dependencies": {
    "qiao-ajax": "^2.8.6",
    "qs": "^6.11.1"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
