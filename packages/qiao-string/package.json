{
  "name": "qiao-string",
  "version": "5.1.2",
  "description": "nodejs string tool",
  "keywords": [
    "nodejs",
    "string",
    "tool"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.vincentqiao.com/#qiao-string",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-z.git"
  },
  "scripts": {
    "build": "qpro rollup ./rollup.config.js"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  },
  "dependencies": {
    "qiao.log.js": "^3.7.3"
  }
}
