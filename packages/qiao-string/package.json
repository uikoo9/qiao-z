{
  "name": "qiao-string",
  "version": "3.0.6",
  "description": "nodejs string tool",
  "keywords": [
    "nodejs",
    "string",
    "tool"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.insistime.com/qiao-string",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-z.git"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
