{
  "name": "qiao-sentry",
  "version": "5.7.7",
  "description": "nodejs sentrys tools",
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.vincentqiao.com/#qiao-sentry",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-z.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-z/issues"
  },
  "scripts": {
    "build": "qpro rollup ./rollup.config.js"
  },
  "dependencies": {
    "@sentry/node": "^10.11.0",
    "@sentry/profiling-node": "^10.11.0"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
