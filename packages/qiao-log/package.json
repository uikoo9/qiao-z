{
  "name": "qiao-log",
  "version": "1.0.8",
  "description": "nodejs log tools",
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.insistime.com/qiao-log",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/qiao-z.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/qiao-z/issues"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "log4js": "^6.7.1"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
