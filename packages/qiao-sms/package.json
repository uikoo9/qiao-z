{
  "name": "qiao-sms",
  "version": "5.0.0",
  "description": "nodejs tencent sms tool",
  "keywords": [
    "tencent",
    "cloud",
    "sms",
    "qcloud",
    "qcloudsms_js"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.vincentqiao.com/#qiao-sms",
  "license": "MIT",
  "main": "index.js",
